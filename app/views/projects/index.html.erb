
<!--displays all the projects and you can edit or or delete-->


<p id="notice"><%= notice %></p>
<div class="row">
      <div class="small-10 medium-10 large-12 small-centered large-centered columns" role="content">
<div class="project">
  <% @projects.each do |project| %>
  <ul>
    <h3><%= link_to project.title, project %></h3>
  </ul>
  <% end %>
</div>

<br>

  <tbody>
    <% if @criticaltasks != nil %>
    <% @criticaltasks.each do |task| %>
      <tr>
        <td><%= task.title %></td>
        <td><%= task.description %></td>
        <td><%= task.status %></td>
        <td><%= task.priority %></td>
        <td><%= task.story_points %></td>
        <td><%= task.deadline %></td>
      </tr>
      <% end %>
      <% else %>
      <%= @sentence %>
    <% end %>
   
  </tbody>
<br>
<%= link_to 'New Project', new_project_path %>
